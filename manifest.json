{
  "manifest_version": 3,
  "name": "Pseudo Refresh",
  "version": "1.1",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon.png",
      "48": "images/icon.png",
      "128": "images/icon.png"
    }
  },
  "permissions": [
    "storage",
    "commands",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "enable_or_disable": {
      "suggested_key": {
        "default": "Ctrl+Shift+E"
      },
      "description": "Capture Ctrl+E"
    }
  },  
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  }
}